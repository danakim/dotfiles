cai-vm() {
    cd ~/workspace/creativeai/creativeai-vm
    vagrant ssh
}

cai-vm-start() {
    cd ~/workspace/creativeai/creativeai-vm
    vagrant up
    vagrant ssh
}

ssh-add -l | grep -iq cai-deployer &> /dev/null
if [ ! $? == 0 ]; then
    ssh-add -k /Users/dan/.ssh/cai-deployer &> /dev/null
fi
